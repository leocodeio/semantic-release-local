{
  "name": "@leocodeio/commit-semantic-release-template",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prepare": "husky",
    "typecheck": "rm -rf dist && tsc",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "release": "semantic-release",
    "commit": "git-cz",
    "zip-dist": "zip -r dist.zip dist",
    "unzip-dist": "unzip dist.zip"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "typescript-eslint": "^8.24.1",
    "commitizen": "^4.3.0",
    "@commitlint/cz-commitlint": "^19.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^11.0.0",
    "semantic-release": "^23.0.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leocodeio/commit-semantic-release-template.git"
  }
}
